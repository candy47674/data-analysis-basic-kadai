        "import numpy as np # インポート\n",
        "A =np.array([   #A配列\n",
        "    [0,1],\n",
        "    [2,3],\n",
        "    [4,5]])\n",
        "B =np.array([   #B配列\n",
        "    [0,1,2,3],\n",
